#!/usr/bin/env node

var program = require('commander')
var package = require('../package.json')

const result = program
  .version(package.version)
  .command('start', 'start moltres daemon')
  .command('stop', 'stop moltres daemon')
  .parse(process.argv)

if (!program.args.length) program.help()
if (result == program) {
  console.log('\n  moltresd: \'' + program.args[0] + '\' is not a command')
  program.help()
}
